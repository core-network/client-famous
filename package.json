{
  "name": "nodesphere-graphviz",
  "version": "0.0.0",
  "description": "Work in progress... look away ;)",
  "authors": [
    "Harlan T Wood <code@nodesphere.org> (http://nodesphere.org/)",
    "Jack Senechal (https://github.com/jacksenechal)"
  ],
  "license": "MIT",
  "scripts": {
    "build": "browserify src/index.coffee -t coffeeify -g uglifyify | uglifyjs --screw-ie8 -m -c dead_code,sequences,conditionals,booleans,unused,if_return,join_vars,drop_debugger > dist/bundle.js",
    "compile": "coffee --output tmp/js/ --compile --bare src/",
    "dev": "npm run watch & http-server",
    "publish": "npm run build && bin/publish",
    "shipit": "npm run test && git push origin head",
    "test": "npm run build",
    "watch": "watchify src/index.coffee -t coffeeify -d -v -o dist/bundle.js"
  },
  "devDependencies": {
    "babelify": "^6.0.1",
    "browserify": "^10.1.3",
    "chalk": "^1.0.0",
    "coffee-script": "^1.9.3",
    "coffeeify": "^1.1.0",
    "eslint": "^0.22.1",
    "famous": "^0.6.2",
    "finalhandler": "^0.3.6",
    "gh-pages": "^0.3.1",
    "jscs": "^1.7.3",
    "uglify-js": "^2.4.15",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodesphere/graphviz.git"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
